{
    "manifest_version": 3,
    "name": "Snavigation-Chrome",
    "version": "1.2",
    "description": "Snavigation 一个简约的起始页",
    "icons": {
        "32": "img/icon/favicon_32.png",
        "64": "img/icon/favicon_64.png",
        "128": "img/icon/favicon_128.png",
        "144": "img/icon/favicon_144.png"
    },
    "chrome_url_overrides": {
        "newtab": "index.html"
    },
    "permissions": [
        "storage",
        "search"
    ]
}